                                       <table>
                                          <tr>
                                             <th class="cAPI-table-name">Name</th>
                                             <th class="cAPI-table-desc">Description</th>
                                          </tr>
                                          {{#each enumerators}}
                                          <tr>
                                             <td class="cAPI-table-name">{{name}}</td>
                                             <td class="cAPI-table-desc">{{{description}}}</td>
                                          </tr>
                                          {{/each}}
                                       </table>
                                       {{/if}} {{/if}}
                                       <!-- Annotations-->
                                       {{#if isAnnotation}}
                                       <h2 id="{{name}}">
                                          <span class="{{icon}} sm-icon"></span>
                                          public annotation {{name}}
                                       </h2>
                                       {{#if description}}
                                       <p>
                                          <span class="fw-right gray xs-icon"></span>{{{description}}}
                                       </p>
                                       {{/if}}
                                       <p>
                                       </p>
                                       <table>
