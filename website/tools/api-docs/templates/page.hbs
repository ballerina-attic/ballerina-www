                                          <span class="{{icon}} sm-icon"></span>
                                          {{name}}
                                       </h2>
                                       <table>
                                          <tr>
                                             <th class="cAPI-table-name">Attribute Name</th>
                                             <th class="cAPI-table-type">Data Type</th>
                                             <th class="cAPI-table-desc">Description</th>
                                          </tr>
                                          <tr>
                                             <td class="cAPI-table-name">{{name}}</td>
                                             <td class="cAPI-table-type"><a href="{{href}}">{{dataType}}</a></td>
                                             <td class="cAPI-table-desc">{{{description}}}</td>
                                          </tr>
                                       </table>
                                       {{/if}}
                                    </div>
                                    {{/each}}
