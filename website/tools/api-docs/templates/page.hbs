                                          </tr>
                                          <tr>
                                             <td class="cAPI-table-name">{{attachments}}</td>
                                             <td class="cAPI-table-type"><a href="{{href}}">{{dataType}}</a></td>
                                          </tr>
                                       </table>
                                       {{/if}}
                                       <!-- Global Variables-->
                                       {{#if isGlobalVariable}}
